  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pagarme/browserstack-automated.git"
  },
  "author": "Pagar.me Pagamentos S/A",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pagarme/browserstack-automated/issues"
  },
  "homepage": "https://github.com/pagarme/browserstack-automated#readme",
  "devDependencies": {
    "ava": "0.24.0",
    "babel": "6.23.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-ramda": "1.4.3",
    "babel-preset-es2015": "6.24.1",
    "eslint": "4.13.1",
    "eslint-config-pagarme-base": "2.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "sinon": "4.1.3",
    "uglifyjs-webpack-plugin": "1.1.4",
    "webpack": "3.10.0"
  },
  "dependencies": {
    "async": "2.6.0",
    "ramda": "0.25.0",
    "wd": "1.5.0"
  },
  "ava": {
    "files": [
      "tests/**/*.test.js"
    ],
