{
  "name": "browserstack-automated",
  "version": "1.0.0",
  "description": "Automating the use of Browser Stack.",
  "main": "index.js",
  "scripts": {
    "build": "yarn build:commonjs",
    "build:commonjs": "webpack",
    "start": "npm run build",
    "lint": "eslint src/**/*.js",
